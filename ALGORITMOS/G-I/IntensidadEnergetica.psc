Proceso IntensidadEnergetica
	Escribir Sin Saltar "Ingrese el valor de consumo energetico:";
	Leer consumo_energetico;
	Escribir Sin Saltar "Ingrese el valor de producto interior bruto:";
	Leer producto_interior_bruto;
	intensidad_energetica <- consumo_energetico/producto_interior_bruto;
	Escribir "Valor de intensidad energetica: ", intensidad_energetica;
FinProceso
