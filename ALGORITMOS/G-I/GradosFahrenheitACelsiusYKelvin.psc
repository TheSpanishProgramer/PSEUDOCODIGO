Proceso GradosFahrenheitACelsiusYKelvin
	Escribir Sin Saltar "Ingrese el valor de grados Fahrenheit:";
	Leer grados_Fahrenheit;
	grados_Celsius <- (grados_Fahrenheit-32)/1.8;
	grados_Kelvin <- grados_Celsius+273.15;
	Escribir "Valor de grados Celsius: ", grados_Celsius;
	Escribir "Valor de grados Kelvin: ", grados_Kelvin;
FinProceso
