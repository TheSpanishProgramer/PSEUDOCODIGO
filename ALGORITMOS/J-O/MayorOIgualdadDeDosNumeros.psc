Proceso MayorOIgualdadDeDosNumeros
	Escribir Sin Saltar "Ingrese el valor de a:";
	Leer a;
	Escribir Sin Saltar "Ingrese el valor de b:";
	Leer b;
	Si a>b Entonces
		Escribir "El mayor es a.";
	FinSi
	Si a = b Entonces
		Escribir "Son iguales.";
	FinSi
	Si b>a Entonces
		Escribir "El mayor es b.";
	FinSi
FinProceso
