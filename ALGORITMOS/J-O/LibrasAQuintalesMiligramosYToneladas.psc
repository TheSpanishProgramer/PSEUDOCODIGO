Proceso LibrasAQuintalesMiligramosYToneladas
	Escribir Sin Saltar "Ingrese el valor de libras:";
	Leer libras;
	quintales <- libras/100;
	miligramos <- libras*453.62;
	toneladas <- libras*0.00045362;
	Escribir "Valor de miligramos: ", miligramos;
	Escribir "Valor de quintales: ", quintales;
	Escribir "Valor de toneladas: ", toneladas;
FinProceso
