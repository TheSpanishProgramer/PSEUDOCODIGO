Proceso OrdenDeDosValores
	Escribir Sin Saltar "Ingrese el valor de a:";
	Leer a;
	Escribir Sin Saltar "Ingrese el valor de b:";
	Leer b;
	Si a<b Entonces
		Escribir "Orden creciente";
	FinSi
	Si a>b Entonces
		Escribir "Orden decreciente";
	FinSi
	Si a = b Entonces
		Escribir "Son iguales";
	FinSi
FinProceso
