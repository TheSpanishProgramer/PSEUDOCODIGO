Proceso MultiplicacionDeSumas
	Escribir Sin Saltar "Ingrese el valor de N1:";
	Leer N1;
	Escribir Sin Saltar "Ingrese el valor de N2:";
	Leer N2;
	Escribir Sin Saltar "Ingrese el valor de N3:";
	Leer N3;
	Escribir Sin Saltar "Ingrese el valor de N4:";
	Leer N4;
	R1 <- (N1+N2)*(N3+N4);
	Escribir "Valor de R1: ", R1;
FinProceso
