Proceso MillasAKilometrosCentimetrosYYardas
	Escribir Sin Saltar "Ingrese el valor de millas:";
	Leer millas;
	yardas <- millas*1760;
	centimetros <- yardas*91.4;
	kilometros <- centimetros/1000000;
	Escribir "Valor de centimetros: ", centimetros;
	Escribir "Valor de kilometros: ", kilometros;
	Escribir "Valor de yardas: ", yardas;
FinProceso
