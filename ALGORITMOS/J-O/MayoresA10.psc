Proceso MayoresA10
	Escribir Sin Saltar "Ingrese el valor de n:";
	Leer n;
	Para i<-1 Hasta n Con Paso 1 Hacer
		Escribir "PROCESO ", i;
		Escribir Sin Saltar "Ingrese el valor de un numero:";
		Leer un_numero;
		Si un_numero>10 Entonces
			Escribir "El valor es mayor a 10.";
		FinSi
		Escribir "";
	FinPara
FinProceso
