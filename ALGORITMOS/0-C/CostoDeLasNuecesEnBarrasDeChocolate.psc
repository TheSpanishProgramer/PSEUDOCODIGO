Proceso CostoDeLasNuecesEnBarrasDeChocolate
	Escribir Sin Saltar "Ingrese el valor de cantidad de barras de chocolate:";
	Leer cantidad_de_barras_de_chocolate;
	onzas_de_nueces <- cantidad_de_barras_de_chocolate*3.0/4.0;
	costo_de_las_nueces <- onzas_de_nueces*0.12;
	Escribir "Valor de costo de las nueces: ", costo_de_las_nueces;
	Escribir "Valor de onzas de nueces: ", onzas_de_nueces;
FinProceso
