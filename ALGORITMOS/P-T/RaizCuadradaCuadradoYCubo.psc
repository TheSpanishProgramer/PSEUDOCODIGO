Proceso RaizCuadradaCuadradoYCubo
	Escribir Sin Saltar "Ingrese el valor de numero entero:";
	Leer numero_entero;
	raiz_cuadrada <- RC(numero_entero);
	cuadrado <- numero_entero*numero_entero;
	cubo <- cuadrado*numero_entero;
	Escribir "Valor de cuadrado: ", cuadrado;
	Escribir "Valor de cubo: ", cubo;
	Escribir "Valor de raiz cuadrada: ", raiz_cuadrada;
FinProceso
