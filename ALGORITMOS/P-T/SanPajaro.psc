Proceso SanPajaro
	Escribir Sin Saltar "Ingrese el valor de un numero:";
	Leer un_numero;
	Si un_numero = 10 Entonces
		Escribir "Excelente";
	FinSi
	Si un_numero = 9 Entonces
		Escribir "Muy Bien";
	FinSi
	Si un_numero = 8 Entonces
		Escribir "Bien";
	FinSi
	Si un_numero = 7 Entonces
		Escribir "Regular";
	FinSi
	Si un_numero = 6 Entonces
		Escribir "Panzaste";
	FinSi
	Si un_numero<=5 Entonces
		Escribir "Ya te llevó San Pájaro";
	FinSi
FinProceso
