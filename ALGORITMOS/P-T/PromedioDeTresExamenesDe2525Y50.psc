Proceso PromedioDeTresExamenesDe2525Y50
	Escribir Sin Saltar "Ingrese el valor de primer examen:";
	Leer primer_examen;
	Escribir Sin Saltar "Ingrese el valor de segundo examen:";
	Leer segundo_examen;
	Escribir Sin Saltar "Ingrese el valor de tercer examen:";
	Leer tercer_examen;
	promedio <- primer_examen*0.25+segundo_examen*0.25+tercer_examen*0.5;
	Escribir "Valor de promedio: ", promedio;
FinProceso
