Proceso TiempoEnLlegarEnBicicleta
	Escribir Sin Saltar "Ingrese el valor de distancia en km:";
	Leer distancia_en_km;
	Escribir Sin Saltar "Ingrese el valor de velocidad en km por h:";
	Leer velocidad_en_km_por_h;
	tiempo_en_h <- distancia_en_km/velocidad_en_km_por_h;
	Escribir "Valor de tiempo en h: ", tiempo_en_h;
FinProceso
