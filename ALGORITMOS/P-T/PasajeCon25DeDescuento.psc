Proceso PasajeCon25DeDescuento
	Escribir Sin Saltar "Ingrese el valor de precio original:";
	Leer precio_original;
	descuento <- precio_original*0.25;
	importe_total <- precio_original-descuento;
	Escribir "Valor de descuento: ", descuento;
	Escribir "Valor de importe total: ", importe_total;
FinProceso
