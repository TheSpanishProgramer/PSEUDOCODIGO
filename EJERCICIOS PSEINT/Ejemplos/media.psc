Algoritmo Media
	Definir nota1, nota2, notaMedia Como Real;;
	
	Escribir Sin Saltar "Por favor, introduzca la nota del primer examen: ";
	Leer nota1;
	
	Escribir Sin Saltar "Por favor, introduzca la nota del segundo examen: ";
	Leer nota2;
	
	notaMedia <- (nota1 + nota2) / 2;
	
	Escribir "La nota media es ", notaMedia;
FinAlgoritmo
